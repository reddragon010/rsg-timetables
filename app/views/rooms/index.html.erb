<h1>Listing rooms</h1>

<table>
  <tr>
    <th>Short</th>
    <th>Name</th>
    <th></th>
    <th></th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%= link_to room.short, room %></td>
    <td><%= room.name %></td>
    <td><%= link_to 'Edit', edit_room_path(room) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Room', new_room_path %>
